def kangaroo(x1, v1, x2, v2):
    jump1 = x1 + v1
    jump2 = x2 + v2
    
    if jump1 == jump2:
        return "YES"
    
    while jump1 < jump2:
        if v2 < v1:
            jump1 = jump1 + v1
            jump2 = jump2 + v2
        else:
            return "NO"
            break
    if jump1 == jump2:
        return "YES"
    else:
        return "NO"
