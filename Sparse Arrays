def matchingStrings(strings, queries):
    count = 0
    results = list()
    for query in queries:
        for string in strings:
            if query == string:
                count += 1
        results.append(count)
        count = 0
    return results
    
    
    
UPDATED:

def matchingStrings(strings, queries):
    count = 0
    results = []
    for query in queries:
        results.append(strings.count(query))
    return results
